<h2 align="center">Change your password</h2>

<%= form_for(resource_name, resource, :url => password_path(resource_name), :html => { :method => :put }) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :reset_password_token %>

  <p align="center"><%= f.label :password %></p>
  <p align="center"><%= f.password_field :password %></p>

  <p align="center"><%= f.label :password_confirmation %></p>
  <p align="center"><%= f.password_field :password_confirmation %></p>

  <p align="center"><%= f.submit "Change my password" %></p>
<% end %>

